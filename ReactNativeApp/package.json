{
  "name": "reactnativeapp",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "p-mazhnik",
    "url": "https://github.com/p-mazhnik"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "install:ios": "cd ios && pod repo update && pod update Flutter FlutterModuleFrameworks-Debug FlutterModuleFrameworks-Release && pod install",
    "web": "webpack serve --mode=development --config web/webpack.config.js --hot",
    "web:build": "rimraf web/dist/ && webpack --mode=production --config web/webpack.config.js",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "upgrade-flutter": "yarn upgrade flutter-module-rn"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "flutter-module-rn": "file:../flutter_module_rn",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-web": "0.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-native-web": "^0.17.5",
    "copy-webpack-plugin": "^10.2.0",
    "eslint": "^7.14.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native-web",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
